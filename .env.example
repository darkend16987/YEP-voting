# Firebase Configuration
# Lấy từ Firebase Console > Project Settings > General > Your apps > Web app

VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# App Configuration
VITE_APP_ID=default-app

# Security Configuration (QUAN TRỌNG: Đổi mã bảo mật trước khi đưa vào production)
VITE_SECURITY_CODE=your_security_code_here
VITE_ADMIN_EMAIL=admin@example.com

# Expected total users (for dashboard progress)
VITE_TOTAL_EXPECTED_USERS=280
